{
  "name": "n8n-nodes-druva-msp",
  "version": "0.1.0",
  "description": "n8n node for Druva MSP API integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-package",
    "druva",
    "msp"
  ],
  "license": "MIT",
  "homepage": "https://github.com/msoukhomlinov/n8n-nodes-druva-msp",
  "author": {
    "name": "Max Soukhomlinov",
    "email": "maxs@intellectit.com.au"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msoukhomlinov/n8n-nodes-druva-msp.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "gulp clean && tsc && gulp build:post-compile",
    "dev": "gulp clean && tsc && gulp build:post-compile && tsc --watch & gulp dev:watch",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DruvaMspApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DruvaMSP/DruvaMsp.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.16",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "del": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "jest": "^29.5.0",
    "n8n-workflow": "^1.70.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
